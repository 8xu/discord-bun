{
  "name": "discord",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run .",
    "dev": "NODE_ENV=development bun run --watch .",
    "build": "bun build . --target=bun --outdir=dist --outfile=index.js",
    "build:prod": "bun build --compile --minify --sourcemap . --outfile ./dist/run"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "discord.js": "^14.14.1"
  }
}